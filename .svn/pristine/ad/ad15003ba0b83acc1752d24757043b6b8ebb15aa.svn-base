{% extends "layout/admin.html" %}
{% block content %}
<div class="row">
    <ol class="breadcrumb">
        <li><a href="/admin/products">产品管理</a></li>
        <li class="active">
            [{{ p.name|default('新建产品') }}]
        </li>
    </ol>
</div>
<div style="color: red; font-size: 0.6em; margin-bottom: 25px;">
    {%set messages=handler.get_flashed_messages() %}
    {%-if messages-%}
    <div id="err_div">
        {% for type, msg in messages%}
        {{msg}}
        {% endfor %}
    </div>
    {%-endif-%}
</div>
<div class="row">
    <form class="form-horizontal" role="form" action="/admin/product/{{p.id|default('0')}}" method="post">
        {{xsrf()}}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">基本信息
                    <button type="submit" class="btn btn-outline btn-primary btn-xs">保存</button>
                    <a href="/admin/collect/{{p.id}}/0" class="btn btn-outline btn-primary btn-xs" style="display:none;">采价</a>
                </h3>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <label class="col-sm-2 control-label">名称</label>
                    <div class="col-sm-6">
                        <input class="form-control txtName" name="pname" type="text" placeholder="名称不能为空"
                               value="{{p.name}}" required>
                    </div>
                    <label class="col-sm-1 control-label">标签</label>
                    <div class="col-sm-3">
                        <input class="form-control txtTags" name="tags" type="text" placeholder="以英文,分隔"
                               value="{{p.tags}}" required>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        SKU
                    </label>
                    <div class="col-sm-6">
                        <input class="form-control" name="psku" type="text"
                               value="{{p.sku}}" {% if p %} disabled {% endif %}>
                    </div>
                    <label class="col-sm-1 control-label">
                        权重
                    </label>
                    <div class="col-sm-3">
                        <input type="text" name="weights" id="weights" value="{{p.weights and p.weights or '0'}}" class="form-control"
                            placeholder="数字,权重用来排序,数字越大,排在越前" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') "/>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        简介
                    </label>
                    <div class="col-sm-10">
                        <input class="form-control" name="presume" type="text" placeholder="简介不能为空"
                               value="{{p.resume}}" required >
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        分类
                    </label>
                    <div class="col-sm-4">
                        <select class="form-control" name="pcategory" id="pcategory">
                            {% for c in categorys %}
                            <option value="{{c.id}}"  {{default_category.pid ==c.id and 'selected' or ''}}>
                            {{c.name}}
                            </option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="col-sm-3">
                        <select class="form-control" name="category" id="category"  placeholder="请选择分类" required>
                        </select>
                    </div>
                    <div class="col-sm-3">
                        <select class="form-control" name="category3" id="category3"  placeholder="请选择分类" required>
                        </select>
                    </div>
                </div>
                <div class="form-group" style="display: none;">
                    <label class="col-sm-2 control-label">
                        采购来源
                    </label>
                    <div class="col-sm-10">
                        <label class="radio-inline">
                            <input type="radio" name="product_from" id="fromA"  value="A" {% if p.args=='A' %} checked  {% endif %} >批发市场
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="product_from" id="fromB"  value="B" {% if p.args=='B' %} checked  {% endif %} >直采
                        </label>
                    </div>
                </div>
                <div class="form-group" style="display:none;">
                    <label class="col-sm-2 control-label">
                        是否订购
                    </label>
                    <div class="col-sm-10">
                        <label class="checkbox-inline col-sm-4">
                            <input type="checkbox" name="is_reserve" id="is_reserve"  value="1" {% if p.is_reserve==1 %} checked  {% endif %} >是否为订购商品
                        </label>
                        <div class="col-sm-8 dg_time" style="{{p.is_reserve != 1 and 'display:none' or ''}}">
                            <div class="col-sm-3">
                                <label class="control-label">统一配送时间</label></div>
                            <div class="col-sm-9">
                                <input type="text" name="reserve_time" id="reserve_time" value="{{p.reserve_time and p.reserve_time|datetimeformat or ''}}" class="Wdate form-control" onClick="WdatePicker()" style="width:105px; height: 34px;" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        是否设为精品
                    </label>
                    <div class="col-sm-10">
                        <label class="checkbox-inline col-sm-6">
                            <input type="checkbox" name="is_recommend" id="is_recommend"  value="1" {% if p.is_recommend==1 %} checked  {% endif %} >是否为精品商品
                        </label>
                        <div class="col-sm-6">
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        商品所属
                    </label>
                    <div class="col-sm-10">
                        <label class="checkbox-inline col-sm-3">
                            <input type="radio" name="is_bargain"  value="0" {% if p.is_bargain==0 %} checked  {% endif %} >普通商品
                        </label>
                        <label class="checkbox-inline col-sm-3">
                            <input type="radio" name="is_bargain"  value="1" {% if p.is_bargain==1 %} checked  {% endif %} >免费商品
                        </label>
                        <label class="checkbox-inline col-sm-3">
                            <input type="radio" name="is_bargain"  value="2" {% if p.is_bargain==2 %} checked  {% endif %} >特价商品
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        单日限购数量
                    </label>
                    <div class="col-sm-10">
                        <label class="col-sm-5">
                            <input type="text" name="xgperusernum" id="xgperusernum" value="{{p.xgperusernum and p.xgperusernum or '0'}}" class="form-control" style="width:205px; height: 34px;"
                                         placeholder="限购数量只允许为数字" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') "/>
                        </label>
                        <div class="col-sm-7">
                            <div class="col-sm-4">
                                <label class="control-label">库存限购总数</label></div>
                            <div class="col-sm-8">
                                <input type="text" name="xgtotalnum" id="xgtotalnum" value="{{p.xgtotalnum and p.xgtotalnum or '0'}}" class="form-control" style="width:205px; height: 34px;"
                                         placeholder="限购数量只允许为数字" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') "/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group" style="display: none;;">
                    <label class="col-sm-2 control-label">
                        日均销量(克)
                    </label>
                    <div class="col-sm-10">
                        <label class="col-sm-5">
                            <input type="text" class="form-control" name="avg_quantity" value="{{p.avg_quantity and p.avg_quantity or '0'}}" style="width:205px; height: 34px;"
                                         placeholder="日平均销售数量只允许为数字" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') ">
                        </label>
                        <div class="col-sm-7 ">
                            <div class="col-sm-4">
                                <label class="control-label">保质期天数</label></div>
                            <div class="col-sm-8">
                                <input type="text" name="bz_days" id="bz_days" value="{{p.bz_days and p.bz_days or '0'}}" class="form-control" style="width:205px; height: 34px;"
                                         placeholder="保质期只允许为数字" onkeyup="this.value=this.value.replace(/[^\d]/g,'') " onafterpaste="this.value=this.value.replace(/[^\d]/g,'') "/>
                            </div>
                        </div>
                    </div>
                </div>
                {% if p %}
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        详细介绍
                    </label>
                    <div class="col-sm-10">
                        <textarea class="form-control" rows="10" name="pintro" id="pintro">{{p.intro}}</textarea>
                    </div>
                </div>
                {%endif%}
                <div class="form-group">
                    <label class="col-sm-2 control-label" >
                        产地
                    </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="pproducer" value="{{p.producer}}">
                    </div>
                </div>
                <div class="form-group">
                    <label  class="col-sm-2 control-label">
                        关键字
                    </label>
                    <div class="col-sm-10">
                        <input class="form-control" rows="3" id="pmetakeywords" name="pmetakeywords" value="{{p.metakeywords}}" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">
                        meta description
                    </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="pmetadescription" value="{{p.metadescription}}">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label" >
                        meta title
                    </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="pmetatitle" value="{{p.metatitle}}">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label" >
                        质量要求
                    </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="quality" value="{{p.quality|null}}">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label" >
                        检验标准
                    </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="standard" value="{{p.standard|null}}">
                    </div>
                </div>
            </div></div>
        {%-if p-%}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">价格信息 <a id="add_price" style="display: none;" href="/admin/editprice/{{p.id}}/0" class="btn btn-outline btn-primary btn-xs">增加</a></h3>
            </div>
            <div class="panel-body">
                {% for c in p.standards %}
                <div class="col-lg-3 .col-xs-6">
                    {%-if p.defaultstandard==c.id -%}
                    <div class="panel panel-yellow">
                        {% else %}
                        <div class="panel panel-default">
                            {% endif %}
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="text-left">
                                        <h4>{{c.name}} [￥{{c.price}}] </h4>
                                        <div>折合每斤：￥{{c.ourprice}} [市场价：{{c.orginalprice}}]</div>
                                        <div></div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-footer">

                                <div class="btn-group btn-group-xs">
                                    <!--<a type="button" class="btn btn-default" href="/admin/delprice/{{c.id}}">删除</a>-->
                                    <a type="button" class="btn btn-default" href="/admin/editprice/{{p.id}}/{{c.id}}">修改</a>
                                    {%-if not p.defaultstandard==c.id -%}
                                    <a type="button" class="btn btn-default" href="/admin/primaryprice/{{c.id}}">设为默认价格</a>
                                    {% endif %}
                                </div>
                            </div>

                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">图片信息 <a href="" id="fileupload">选择图片</a></h3>
                </div>
                <div class="panel-body" id="pic-box">
                    {% for c in p.pics -%}
                    <div class="col-lg-2 .col-xs-12">
                        {%-if p.cover==c.path -%}
                        <div class="panel panel-primary">
                            {% else %}
                            <div class="panel panel-default">
                                {% endif %}
                                <div class="panel-heading center-block">
                                    <img src="{{c.path}}" width="80" height="80" style="cursor: pointer;" onclick="window.open(this.src)">
                                </div>
                                <div class="panel-footer">
                                    <div class="btn-group btn-group-xs">
                                        <a type="button" class="btn btn-default" href="/admin/delpic/{{c.id}}">删除</a>
                                        {%-if not p.cover==c.path -%}
                                        <a type="button" class="btn btn-default" href="/admin/primarypic/{{c.id}}">设为主图</a>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>

                        </div>
                        {%- endfor %}
                    </div>
                </div>
                {%-endif-%}


    </form>
</div>
{% endblock %}
{% block js %}
<script src="/style2/js/jquery-1.7.2.min.js?v=1"></script>
<script src="/style2/xheditor/xheditor-1.2.2.min.js"></script>
<script src="/style2/xheditor/xheditor_lang/zh-cn.js"></script>
<script src="/style2/js/jquery.ocupload.js?v=1"></script>
<script language="javascript" type="text/javascript" src="/style2/js/My97DatePicker/WdatePicker.js"></script>
<script>
    $(function(){
        var xsrf='{{handler.xsrf_token}}';
        $("#pcategory").change(function(){
//            debugger;
            var pid = $(this).val();
            $.get("/ajax/get_sub_category", { _xsrf : xsrf , pid : pid}, function (data) {
                data = jQuery.parseJSON(data);
                var optionstring = "";
                if(data.flag == 1) {
                    for (var j = 0; j < data.data.length; j++) {
                        optionstring += "<option value=\"" + data.data[j].id + "\" >" + data.data[j].name + "</option>";
                    }
                    $("#category").html("<option value=''>请选择...</option> " + optionstring);
                }
            });
        });
        $("#pcategory").change();
        $("#is_reserve").click(function(){
            if(this.checked){
                $('.dg_time').show();
            }else{
                $('.dg_time').hide();
                $("#reserve_time").val("");
            }
        });

        $('#pintro').xheditor({tools:'Source,Fullscreen,Img', html5Upload:false, upImgUrl:"/ajax/upload/{{p.sku}}",upImgExt:"jpg,gif,png"});

        var myUpload = $('#fileupload').upload({
            name: 'filedata',
            action: '/ajax/product/pic/{{p.id}}',
            enctype: 'multipart/form-data',
            autoSubmit: true,
            onClick: function(){

            },
            onSelect: function(){
                var ext = myUpload.filename().substr(myUpload.filename().length - 3, 3).toLowerCase();
                if (ext != 'jpg' || ext != 'gif' || ext != 'png')
                {
                    return false;
                }
            },
            onComplete: function(response) {
                eval('var c = ' + response);
                if (c.id > 0) {

                    $('#pic-box').append('<div class="col-lg-2 .col-xs-12"><div class="panel panel-default"><div class="panel-heading center-block"><img src="/upload/' + c.path + '" width="80" height="80"></div><div class="panel-footer"><div class="btn-group btn-group-xs"><a type="button" class="btn btn-default" href="/admin/delpic/'+c.id+'">删除</a><a type="button" class="btn btn-default" href="/admin/primarypic/'+c.id+'">设为主图</a></div></div></div></div>');

                }
                else{
                    alert(c.path);
                }

            }
        });
        query();
        $(".txtName").change(function(){
            query();
        });

        $("#pcategory").change(function(){
            var pid = $(this).val();
            $.get("/ajax/get_sub_category", { _xsrf : xsrf , pid : pid}, function (data) {
                data = jQuery.parseJSON(data);
                var optionstring = "";
                if(data.flag == 1) {
                    for (var j = 0; j < data.data.length; j++) {
                        optionstring += "<option value=\"" + data.data[j].id + "\" >" + data.data[j].name + "</option>";
                    }
                    $("#category").html("<option value=''>请选择...</option> " + optionstring);
                    {% if default_category2 %}
                        $("#category option[value='{{default_category2}}']").attr("selected", true);
                        $("#category").change();
                    {% endif %}
                }
            });
        });
        $("#category").change(function(){
            var pid = $(this).val();
            $.get("/ajax/get_sub_category", { _xsrf : xsrf , pid : pid}, function (data) {
                data = jQuery.parseJSON(data);
                var optionstring = "";
                if(data.flag == 1) {
                    for (var j = 0; j < data.data.length; j++) {
                        optionstring += "<option value=\"" + data.data[j].id + "\" >" + data.data[j].name + "</option>";
                    }
                    $("#category3").html("<option value=''>请选择...</option> " + optionstring);
                    {% if default_category3 %}
                        $("#category3 option[value='{{default_category3}}']").attr("selected", true);
                    {% endif %}
                }
            });
        });
        {% if p %}
            $("#pcategory option[value='{{default_category}}']").attr("selected", true);
        {% endif %}
        $("#pcategory").change();

    });
    function query(){
        var str = $(".txtName").val().trim();
        if(str == "") return;
        var arrRslt = makePy(str);
        $(".txtTags").val(arrRslt);
    }
