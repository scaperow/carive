{% block js %}
<script>
    $(function(){
        $("#settings-submit").click(function(){
            var price = $("#txtprice").val();
            if(price != ""){
                if (parseFloat(price) > 0){
                    $("#topupform").submit();
                }else {
                    alert("充值金额必须大于0！");
                }
            }else{
                alert("请输入充值金额！");
            }
        });
    });
</script>
