/* CSS Document */
body { padding:0px; margin:0px; background-color:#48ad67;  font-family:"微软雅黑"; background-image:url(/style2/images/activity/store/bg.jpg); background-position:left center; background-repeat:repeat; padding-top:70px; }

.bt_bg_kz { background-image:url(/style2/images/activity/store/BT.jpg); background-position:center bottom; background-repeat:no-repeat; min-height:880px;}

.map {width:950px; margin:0px auto;}
.map_title { background-image:url(/style2/images/activity/store/title_bg.png); background-position:center bottom; width:px; height:95px; background-repeat:no-repeat; padding-top:30px; }
.map_title_kz { font-weight:bold; font-size:36px; color:#ffffff; margin:0px 0px 0px 360px;}
.map_title_kz_pic  { font-weight:bold; font-size:36px; color:#ffffff; margin:0px 0px 0px 500px;}



