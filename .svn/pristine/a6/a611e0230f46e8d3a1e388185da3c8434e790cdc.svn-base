a img { border:none;}


.copy {width:950PX;margin:0px auto; color:#0b0a0a; font-size:14px; text-align:center; margin-top:140px; }
.copy a {color:#0b0a0a; text-decoration:none; margin-bottom:20px; height:30px;line-height:30px;}
.copy a:hover { text-decoration:underline; }
