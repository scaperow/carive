    border-bottom-color: #008D3E;
    border-right-color: #008D3E;
    padding:6px 10px 6px 10px;
    border-radius:3px;
}
.summary a.favorite{
    position: absolute;
    right: 60px;
    top: 0px;
    background: #DEDEDE;
    border: 1px solid #008D3E;
    font-weight: normal;
    background-image: linear-gradient(to bottom,#f7f7f7,#f4f4f4);
    color: #666;
    border-bottom-color: #008D3E;
    border-right-color: #008D3E;
    padding:6px 10px 6px 10px;
    border-radius:3px;
    cursor: pointer;
}
.properties{
