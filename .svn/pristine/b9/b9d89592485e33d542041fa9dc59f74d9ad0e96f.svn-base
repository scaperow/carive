.summary{
    margin:10px 0px 10px 0;
    position:relative;
    padding:30px;
    min-height:320px;
    border:1px solid #ccc;
    float: left;
}
.summary h3, .summary p, .properties, .services, .operate-box, .authority-list,.service-title,.service-content,.summary .operate{
    margin-left: 380px;
    margin-bottom: 10px;
    position: relative;
}

.summary h3{
    font-size: 20px;
    line-height: 24px;
    font-family: Microsoft YaHei;
    font-weight:normal
}
.summary p span{
    position: absolute;
    left: 95px;
    top: 0;
    color: #999;
    border-left: 1px solid #ccc;
    padding-left: 10px;
}
.summary p span.credit{
    position: absolute;
    left: 185px;
    top: 0;
    color: #999;
    border-left: 1px solid #ccc;
    padding-left: 10px;
}
.summary p span.credit b{
    color: red;
}
.summary p.introduction{
    line-height:20px;
    color:#666;
    border-top:1px solid #efefef;
