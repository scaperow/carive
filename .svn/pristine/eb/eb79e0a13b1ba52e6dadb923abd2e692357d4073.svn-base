.time_old {font-family:"微软雅黑"; font-size:26px;font-weight:bold;color:#000; }
.time_old span {color:#e40001;}

.xline { font-family:"微软雅黑"; font-size:36px;color:#fc6767; margin-bottom:50px; margin-top:20px; }
.rule { font-family:"微软雅黑"; line-height:30px; font-size:18px; border:dashed 2PX #ff0000; BORDER-RAdius:5px; margin-top:0px; padding:60px 20px 20px 40px; z-index:0;}
.rule span { font-weight:bold; color:#e40001; font-size:18px}
.rule_title { background-image:url(/style2/images/activity/reserve/rule_title.jpg); background-position:center top; background-repeat:no-repeat; height:69px;  left:0px; top:40px; position:relative }


.gg { margin-top:5px; text-align:right;font-family:"微软雅黑"; font-size:20px; line-height: 35px;}
.gg_sm  { margin-top:20px; text-align:right;font-family:"微软雅黑"; font-size:18px; color:#838384 }
.rule b {color:#e40001;}
.gg del {text-decoration:line-through; color:#aeadb4}
.price { margin-top:21px; text-align:LEFT; width:270px;}
.yj span { color:#e40001;}
.yjdq  span { color:#e40001;}
.yjdq_sm { font-size:20px; text-align:right; padding-top:0px; margin-bottom:5px;}
.yjdq_sm  span { color:#e40001;}
.yj { font-size:20px; text-align:right; padding-top:20px;}
*+HTML .yj { font-size:20px; text-align:right; padding-top:0px;}
.Order_btn  { text-align:right; width:275PX; height:60px;}

.btn_submit { background-image:url(/style2/images/activity/reserve/btn.jpg); width:185px; height:53px; display:block; border:none; float:right}
