.GRAY_BOX a { color:#000; text-decoration:none}
.GRAY_BOX a:hover { color:#000; text-decoration:underline}

.order { color:#100d0d; width:290PX; text-align:right}
.title { font-family:"微软雅黑"; font-size:36px; font-weight:bold; border-bottom:solid 1px #000; margin-bottom:10px;}
