.copy { color:#050404; font-size:12px; width:1000px; margin:0px auto; text-align:center; font-family:"微软雅黑"; padding-top:20px;}
.copy a { color:#050404; text-decoration:none}
.copy a:hover {text-decoration: underline}
.bt_link { margin-bottom:10px;}

.buy_number_input, .add_cart.buyer_button .unsellable, .track_click, .ilikewrap {
	background: url(/style2/images/mall_product_v4.png) no-repeat;
}
.buy_number_input {
	height: 28px;
	background-position: -198px 0;
	float: left;
	width: 105px;
	display: inline;
	margin-left: 5px;
}
.buy_number_input span {
	display: inline;
	float: left;
	height: 28px;
	width: 28px;
	cursor: pointer;
}
.buy_number_input input {
	float: left;
	display: inline;
	width: 49px;
	height: 28px;
	text-align: center;
	line-height: 28px;
	border: 0 none;
	overflow: hidden;
	background: transparent;
	margin-top: 1px;
	font-size: 16px;
}




