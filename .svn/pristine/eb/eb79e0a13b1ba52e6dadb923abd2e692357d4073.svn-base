/* CSS Document */
body { padding:0px; margin:0px; background-color:#d9ad82;  font-family:"微软雅黑";  background-position:center TOP;  background-repeat:no-repeat;background-position:center top; padding-bottom:20PX;background-image:url(/style2/images/activity/reserve/bg_pi.jpg); background-repeat:repeat;}
.content { padding:0px; margin:0px; background-color:#ecc030;  font-family:"微软雅黑";  background-position:center TOP; background-repeat:no-repeat;
    padding-top:276px; background-image:url(/style2/images/activity/reserve/top_bg.jpg); background-repeat:no-repeat;
    background-position:center top; padding-bottom:20PX;}
a img { border:none;}

.box { background-color:#FFF; width:983px; min-height:900px; margin:0PX AUTO; padding:50px 35px 50px 35px;
background-image:url(/style2/images/activity/reserve/box_vt.jpg); background-position:center bottom; background-repeat:no-repeat;position:relative;border-radius:10px;}

.box_sm { background-color:#FFF; width:903px; min-height:900px; margin:0PX AUTO; padding:90px 75px 50px 75px;background-image:url(/style2/images/activity/reserve/box_vt.jpg); background-position:center bottom; background-repeat:no-repeat;position:relative;border-radius:10px; }
