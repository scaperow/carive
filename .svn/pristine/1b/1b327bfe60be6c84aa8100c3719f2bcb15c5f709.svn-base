<ion-view>
    <ion-content>
        <ion-list style="padding: 0; margin:0;border-width:0px;">
            <ion-item ng-repeat="item in pics" style="padding: 0; margin:0; border-width:0px;">
                <img ng-src="{{item.img}}" style="width:100%;padding: 0; margin:0;border-width:0px;">
            </ion-item>
        </ion-list>
    </ion-content>
</ion-view>
