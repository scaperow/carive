.help_contain .title h1 {
color: #333;
font-size: 46px;
text-align: center;
display: block;
-webkit-margin-before: 0.5em;
-webkit-margin-after: 0.5em;
-webkit-margin-start: 0px;
-webkit-margin-end: 0px;
font-weight: bold;
}
.help_contain .title_line {
background: #fff;
margin-top: 10px;
height: 3px;
    border: dashed 1px #ccc;
}
.help_contain h3 {
font-size: 16px;
padding: 0;
display: block;
